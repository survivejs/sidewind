{
  "name": "sidewind",
  "version": "7.2.2",
  "description": "Sidewind - Tailwind but for state",
  "main": "dist/sidewind.umd.development.js",
  "typings": "dist/index.d.ts",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/survivejs/sidewind.git"
  },
  "bugs": {
    "url": "https://github.com/survivejs/sidewind/issues"
  },
  "homepage": "https://sidewind.js.org/",
  "scripts": {
    "build": "tsdx build --format umd",
    "install_gustwind": "deno install -A --unstable --no-check -f https://deno.land/x/gustwind@v0.17.3/cli.ts",
    "site:build": "gustwind -b",
    "vercel:build": "deno run -A --unstable --no-check https://deno.land/x/gustwind@v0.17.3/cli.ts -b",
    "start": "gustwind -d",
    "lint": "tsdx lint src test",
    "test": "tsdx test",
    "prepare": "npm run build"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "state-management",
    "tailwind"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Juho Vepsäläinen",
  "license": "MIT"
}
