{
  "scripts": {
    "build": {
      "cmd": "deno run -A scripts/buildNpm.ts"
    },
    "watch:build": {
      "cmd": "deno run -A scripts/buildNpm.ts",
      "watch": true
    },
    "install:gustwind": "deno install -A --unstable --no-check -f https://deno.land/x/gustwind@v0.13.7/cli.ts",
    "build:site": "gustwind -b",
    "start": "gustwind -d",
    "vercel:build": "deno run -A --unstable --no-check https://deno.land/x/gustwind@v0.13.7/cli.ts -b"
  }
}
